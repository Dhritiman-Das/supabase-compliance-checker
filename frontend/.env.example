# Environment setup
NODE_ENV=development

# GitHub OAuth credentials (replace with actual keys when configuring)
GITHUB_ID=your_github_id
GITHUB_SECRET=your_github_secret

# NextAuth configuration
NEXTAUTH_SECRET=your_nextauth_secret
NEXTAUTH_URL=http://localhost:3000

# JWT secret key (replace with your secure key)
JWT_SECRET_KEY=your_jwt_secret_key

# Backend and frontend URLs for local development
NEXT_PUBLIC_BACKEND_URL=http://localhost:3333
NEXT_PUBLIC_FRONTEND_URL=http://localhost:3000
NEXT_PUBLIC_DOMAIN=localhost

# OpenAI API key (replace with your actual key)
OPENAI_API_KEY=your_openai_api_key

# MongoDB connection string (local development setup)
MONGODB_URI=mongodb+srv://<db_username>:<db_password>@cluster0.ce3vu.mongodb.net/?retryWrites=true&w=majority&appName=Cluster0

# Support email (replace with your actual support email)
NEXT_PUBLIC_SUPPORT_EMAIL=support@example.com

# Email server configuration for transactional emails (replace with actual credentials)
EMAIL_SERVER_USER=email_user
EMAIL_SERVER_PASSWORD=email_password
EMAIL_SERVER_HOST=smtp.example.com
EMAIL_SERVER_PORT=465
EMAIL_FROM=noreply@example.com
